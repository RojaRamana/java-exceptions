import java.awt.*;
import java.applet.*;

/*
<applet code="GraphicsDemo" width=500 height=300>
</applet>
*/

public class GraphicsDemo extends Applet{
public void paint(Graphics g){
setBackground(Color.pink);
setForeground(Color.black);
int x[]={10,100,10,100,10};
int y[]={20,20,100,100,20};
g.drawLine(10,10,100,10);
g.drawRect(230,120,90,90);
g.fillRect(120,20,90,90);
g.fillRoundRect(230,20,90,90,15,15);
g.drawOval(120,140,90,50);
g.fillOval(10,120,90,90);
g.drawPolygon(x,y,5);
}
}
